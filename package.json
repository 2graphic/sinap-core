    "build": "webpack",
    "test": "mocha test/build/index.js"
  },
  "dependencies": {
    "typescript": "https://github.com/2graphic/sinap-core/blob/typescript-types/typescript-2.2.0.tgz?raw=true"
  },
  "devDependencies": {
    "@types/node": "latest",
    "mocha": "^3.2.0",
    "raw-loader": "0.5.1",
    "ts-loader": "2.0.0",
    "typescript-formatter": "latest",
    "webpack": "2.2.1",
    "webpack-merge": "2.6.1",
    "webpack-node-externals": "1.5.4"
  }
}
